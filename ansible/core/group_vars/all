---
# Defines the region where the server resides. This is used for configuring the AWS CLI tools. The region
# specified here is where the teamcity server config will be backed up to.
#
# This value will normally come from packer, as it will specify a region on the command line.
aws_region: us-west-1

# Sets the Java version installed on the nodes.
oracle_java_version: 8

# Sets the Java minor version installed on the nodes.
oracle_java_version_update: 101

# Use Oracle JDK by default.
oracle_java_set_as_default: yes

# Invalidates the ppa cache after this amount of time.
launchpad_ppa_webupd8_cache_valid_time: 3600

core_update_bucket: webrtc
core_update_object: core
